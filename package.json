{
  "name": "@shun-shobon/resume",
  "version": "0.1.1",
  "author": "Shuntaro Nishizawa <me@s2n.tech> (https://s2n.tech)",
  "license": "MIT",
  "packageManager": "pnpm@8.6.3",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "types": "./dist/mod.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run gen:file",
    "gen:file": "tsx src/bin/gen-file.ts",
    "gen:schema": "tsx src/bin/gen-schema.ts",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint --ext .ts,.tsx src/",
    "lint:prettier": "prettier --check src/",
    "lint-fix": "run-s -c lint-fix:*",
    "lint-fix:eslint": "pnpm run lint:eslint --fix",
    "lint-fix:prettier": "prettier --write src/"
  },
  "devDependencies": {
    "@shun-shobon/eslint-config-base": "^0.4.12",
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "^18.16.18",
    "@types/prettier": "^2.7.3",
    "eslint": "^8.43.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "yaml": "^2.3.1",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.2",
    "zod-to-ts": "^1.1.4"
  }
}
